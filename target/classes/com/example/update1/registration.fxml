<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.update1.RegistrationController">
    <Label layoutX="145" layoutY="14" text="Registration Form" />
    <Label layoutX="37" layoutY="58" text="Username" />
    <TextField fx:id="usernameField" layoutX="118" layoutY="54" promptText="Enter your username" />
    <Label layoutX="37" layoutY="98" text="Email" />
    <TextField fx:id="emailField" layoutX="118" layoutY="94" promptText="Enter your email" />
    <Label layoutX="37" layoutY="138" text="Password" />
    <PasswordField fx:id="passwordField" layoutX="118" layoutY="134" promptText="Enter your password" />
    <Button fx:id="registerButton" layoutX="163.0" layoutY="174.0" onAction="#registerButtonClicked" text="Register" />
   <Button layoutX="137.0" layoutY="221.0" mnemonicParsing="false" onAction="#changeToLogin" prefHeight="25.0" prefWidth="127.0" text="Go to login page" />
</AnchorPane>
